<web-app>
<welcome-file-list>
<welcome-file>index.html</welcome-file>
</welcome-file-list>
<servlet>
<servlet-name>register</servlet-name>
<servlet-class>com.servlets.Registration</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>register </servlet-name>
<url-pattern>/reg</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>login</servlet-name>
<servlet-class>com.servlets.Login</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>login </servlet-name>
<url-pattern>/login</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>wel</servlet-name>
<servlet-class>com.servlets.UserHome</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>wel </servlet-name>
<url-pattern>/welcomePage</url-pattern>
</servlet-mapping>




</web-app>